{
  "name": "@bodiless/knapsack-renderer",
  "description": "Render Bodiless templates in Knapsack",
  "version": "1.0.0-rc.25",
  "type": "commonjs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js"
    },
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:lib && npm run build:api-doc",
    "build:lib": "tsc -p ./tsconfig.json",
    "build:api-doc": "typedoc --options ../../typedoc.js --out doc/api src",
    "build:watch": "npm run build:lib -- --watch",
    "start": "npm run build -- --watch",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src ./tests",
    "test": "ava",
    "clean": "rimraf \"lib/*\" && rimraf tsconfig.tsbuildinfo && rimraf \"doc/api\""
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.16.0",
    "@knapsack/app": "^3.105.5",
    "@knapsack/core": "^3.105.5",
    "@knapsack/file-utils": "^3.105.5",
    "@knapsack/renderer-react": "^3.105.5",
    "@knapsack/renderer-webpack-base": "^3.105.5",
    "@knapsack/types": "^3.105.5",
    "@knapsack/utils": "^3.105.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@bodiless/fclasses": "^1.0.0-rc.25",
    "@knapsack/test-ava": "^3.105.5",
    "@knapsack/typescript-config-starter": "^3.105.5",
    "@types/babel__core": "^7.1.16",
    "@types/node": "^16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "ava": "^3.15.0",
    "ts-expect": "^1.3.0",
    "typescript": "^4.0.0"
  },
  "files": [
    "/lib"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "https://github.com/johnsonandjohnson/Bodiless-JS",
    "directory": "packages/knapsack-renderer",
    "type": "git"
  }
}
